<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.ounwan.guest">

    <insert id="addGuest" parameterType="guests">
    	INSERT INTO
    	GUESTS(EMAIL, PHONE)
    	VALUES(#{email}, #{phone})
    </insert>


</mapper>